    "babel-eslint": "^7.2.3",
    "babel-plugin-ramda": "^1.2.0",
    "babel-preset-es2015": "^6.24.1",
    "np": "^2.16.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "ramdasauce": "^2.0.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-uglify": "^2.0.1",
    "standard": "^10.0.2",
    "typescript": "^2.3.4"
  },
  "files": [
