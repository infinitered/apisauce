{
  "name": "apisauce",
  "version": "0.11.0",
  "description": "Axios + standardized errors + request/response transforms.",
  "repository": {
    "type": "git",
    "url": "https://github.com/skellock/apisauce.git"
  },
  "main": "./dist/apisauce.js",
  "scripts": {
    "test": "ava -s",
    "coverage": "nyc ava",
    "dist": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "build": "BABEL_ENV=production rollup -c",
    "lint": "standard lib/* test/* rollup.config.js",
    "shipit": "npm run dist && np"
  },
  "keywords": [
    "axios",
    "api",
    "network",
    "http"
  ],
  "author": "Steve Kellock <steve@kellock.ca>",
  "license": "MIT",
  "files": [
    "dist/apisauce.js"
  ],
  "dependencies": {
    "axios": "^0.16.1",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "fast-async": "^6.2.2",
    "np": "^2.13.2",
    "nyc": "^10.2.0",
    "ramdasauce": "^1.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-uglify": "^1.0.2",
    "standard": "^10.0.2"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
